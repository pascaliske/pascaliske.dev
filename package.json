{
  "name": "pascaliske-dev",
  "description": "Angular Web App built with GitHub Actions and deployed with Cloudflare.",
  "version": "2.3.6",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "ngcc",
    "lighthouse": "lighthouse --quiet --chrome-flags=\"--headless\"",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "start": "ng serve --ssl",
